{
  "name": "brdata",
  "version": "1.0.0",
  "description": "Gerador de dados contextualizados com a realidade brasileira.",
  "engines": {
    "sketch": ">=3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonathasbsouza/brdata.git"
  },
  "skpm": {
    "name": "BRData",
    "manifest": "src/manifest.json",
    "identifier": "brdata.plugin",
    "appcast": ".appcast.xml",
    "main": "brdata.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  },
  "author": "JÃ´nathas Souza <jonathasbsouza@gmail.com>"
}
